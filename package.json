{
  "name": "vscode-ui-scripting",
  "displayName": "VSCode UI Scripting",
  "publisher": "DruidWorksStudio",
  "description": "This extension allows us to run scripting based on inputs & configs recieved from VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.scriptUI"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.scriptUI",
        "title": "Start: VSCode UI Scripting"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceScheme == file",
          "command": "extension.scriptUI",
          "group": "7_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "5.2.6",
    "@types/node": "11.9.4",
    "tslint": "5.12.1",
    "typescript": "3.3.3",
    "vscode": "1.1.29"
  }
}
